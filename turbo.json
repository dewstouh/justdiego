{
  "$schema": "https://turborepo.com/schema.json",
  "globalEnv": [
    "ADMIN_EMAIL",
    "ADMIN_PASSWORD",
    "RESEND_API_KEY",
    "DATABASE_URL"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ],
      "env": [
        "ADMIN_EMAIL",
        "ADMIN_PASSWORD",
        "RESEND_API_KEY",
        "DATABASE_URL"
      ]
    },
    "start": {
      "cache": false,
      "persistent": true,
      "env": [
        "ADMIN_EMAIL",
        "ADMIN_PASSWORD",
        "RESEND_API_KEY",
        "DATABASE_URL"
      ]
    },
    "db:migrate:dev": {
      "cache": false,
      "persistent": true
    },
    "db:migrate:deploy": {
      "cache": false
    },
    "db:migrate:reset": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "generate": {
      "dependsOn": [
        "^generate"
      ],
      "cache": false
    },
    "lint": {}
  }
}